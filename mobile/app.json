{
  "expo": {
    "name": "PackTrack",
    "slug": "packtrack",
    "scheme": "packtrack",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#15803D"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#15803D"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.packtrack.mobile",
      "versionCode": 2,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow PackTrack to use your camera to scan barcodes and QR codes."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "44bd0532-b30c-4929-a8c2-25525103647c"
      }
    },
    "owner": "halfcrazyph"
  }
}
